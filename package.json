{
  "name": "succubus.space-cli",
  "version": "0.3.6",
  "description": "a CLI tool to hentai made in Typescript",
  "author": "Turbo (turbo.ooo)",
  "contributors": [
    "wuon"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "bin": {
    "succ": "./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TurboCheetah/succubus.space-cli.git"
  },
  "bugs": {
    "url": "https://github.com/TurboCheetah/succubus.space-cli/issues"
  },
  "homepage": "https://github.com/TurboCheetah/succubus.space-cli#readme",
  "keywords": [
    "hentai",
    "anime",
    "cli",
    "succubus.space"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "start": "tsc && node ./lib/index.js",
    "clean": "rm -rf ./node_modules ./package-lock.json && npm install",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "postinstall": "npm run build && node ./lib/scripts/postinstall.js"
  },
  "devDependencies": {
    "@types/7zip-min": "1.3.1",
    "@types/command-exists": "1.2.0",
    "@types/inquirer": "8.1.3",
    "@types/node": "16.11.15",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.5.1",
    "ts-node": "10.4.0",
    "tslint": "5.20.1",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "7zip-min": "^1.4.0",
    "add": "^2.0.6",
    "chalk": "^4.1.2",
    "command-exists": "^1.2.9",
    "got": "^12.0.0",
    "graphql": "^16.2.0",
    "graphql-request": "^3.7.0",
    "inquirer": "^8.2.0",
    "ora": "^5.4.1"
  }
